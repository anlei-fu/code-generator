    <select id="getDetailPage" resultType="com.jasmine.crud_project.pojo.resp.PFrontendConfigDetailResp" parameterType="com.jasmine.crud_project.pojo.req.GetPFrontendConfigPageReq">
          select
             t.content,
             t.create_time,
             t.id,
             t.last_backup_time,
             t.project_id,
             t.update_time,
             t1.project_id as p_project_config_project_id
          from p_frontend_config t
