<mapper namespace="com.jasmine.crud.mapper.AditionalOptionMapper">
    <select id="getDetailPage" resultType="com.jasmine.crud.pojo.resp.AditionalOptionDetailResp" parameterType="com.jasmine.crud.pojo.req.GetAditionalOptionPageReq">
          select
             t.check,
             t.description,
             t.enable,
             t.id,
             t.name,
             t.notice_text,
             t.table_id,
             t.title,
             t1.name as table_name
          from aditional_option t
          left join table_info t1 on  t.table_id = t1.id
          <where>
            <if test="name != null and name != ''">
                  t.name like concat('%',#{name},'%') 
            </if>
          </where>
    </select>
</mapper>
