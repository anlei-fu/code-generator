<mapper namespace="com.jasmine.crud.mapper.TableInfoMapper">
    <select id="getDetailPage" resultType="com.jasmine.crud.pojo.resp.TableInfoDetailResp" parameterType="com.jasmine.crud.pojo.req.GetTableInfoPageReq">
          select
             t.api_prefix,
             t.batch_delete,
             t.batch_eidit,
             t.deletable,
             t.description,
             t.eiditable,
             t.id,
             t.insertable,
             t.moudule_id,
             t.name,
             t.project_id,
             t.raw_name,
             t.title,
             t1.name as project_name
          from table_info t
          left join project t1 on  t.project_id = t1.id
          <where>
            <if test="name != null and name != ''">
                  t.name like concat('%',#{name},'%') 
            </if>
          </where>
    </select>
</mapper>
