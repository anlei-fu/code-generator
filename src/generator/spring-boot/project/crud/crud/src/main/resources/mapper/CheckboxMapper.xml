<mapper namespace="com.jasmine.crud.mapper.CheckboxMapper">
    <select id="getDetailPage" resultType="com.jasmine.crud.pojo.resp.CheckboxDetailResp" parameterType="com.jasmine.crud.pojo.req.GetCheckboxPageReq">
          select
             t.id,
             t.project_id,
             t.content,
             t.update_time,
             t1.id as project_config_id
          from checkbox t
          left join project_config t1 on  t.project_id = t1.id
          <where>
            <if test="startTime != null">
                  t.update_time >= #{startTime}
            </if>
            <if test="endTime != null">
                  and t.update_time &lt; #{endTime}
            </if>
          </where>
    </select>
</mapper>
