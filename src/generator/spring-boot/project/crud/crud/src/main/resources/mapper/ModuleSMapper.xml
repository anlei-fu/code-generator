<mapper namespace="com.jasmine.crud.mapper.ModuleSMapper">
    <select id="getDetailPage" resultType="com.jasmine.crud.pojo.resp.ModuleSDetailResp" parameterType="com.jasmine.crud.pojo.req.GetModuleSPageReq">
          select
             t.description,
             t.id,
             t.level,
             t.name,
             t.parent_moudule_id,
             t.project_id,
             t1.name as project_name
          from module_s t
          left join project t1 on  t.project_id = t1.id
          <where>
            <if test="level != null">
                  t.level = #{level}
            </if>
            <if test="name != null and name != ''">
                  and  t.name like concat('%',#{name},'%') 
            </if>
          </where>
    </select>
</mapper>
