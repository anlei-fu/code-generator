<mapper namespace="com.jasmine.crud.mapper.DictionaryMapper">
    <select id="getDetailPage" resultType="com.jasmine.crud.pojo.resp.DictionaryDetailResp" parameterType="com.jasmine.crud.pojo.req.GetDictionaryPageReq">
          select
             t.id,
             t.name,
             t.type,
             t.value
          from dictionary t
          <where>
            <if test="name != null and name != ''">
                  t.name like concat('%',#{name},'%') 
            </if>
            <if test="type != null">
                  and t.type = #{type}
            </if>
          </where>
    </select>
</mapper>
