<mapper namespace="com.jasmine.crud.mapper.ProjectMapper">
    <select id="getDetailPage" resultType="com.jasmine.crud.pojo.resp.ProjectDetailResp" parameterType="com.jasmine.crud.pojo.req.GetProjectPageReq">
          select
