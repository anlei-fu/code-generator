<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.railway.mapper.DetectGoupUserMapper">
    <insert id="addDetectGoupUser" parameterType="com.railway.pojo.req.AddDetectGoupUserReq">
          insert  into detect_goup_user t
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="groupId != null">
                  t.group_id,
            </if>
             t.is_delete,
            <if test="memberId != null">
                  t.member_id,
            </if>
            <if test="operator != null">
                  t.operator,
            </if>
            <if test="status != null">
                  t.status,
            </if>
             t.create_time
          </trim>
          values
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="groupId != null">
                  #{groupId},
            </if>
             #{isDelete},
            <if test="memberId != null">
                  #{memberId},
            </if>
            <if test="operator != null">
                  #{operator},
            </if>
            <if test="status != null">
                  #{status},
            </if>
             current_timestamp
          </trim>
    </insert>
    <delete id="deleteDetectGoupUserById">
          delete from detect_goup_user t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </delete>
    <update id="updateDetectGoupUserById" parameterType="com.railway.pojo.param.UpdateDetectGoupUserByIdParams">
        update detect_goup_user t
        <set>
            <if test="groupId != null">
                  t.group_id = #{GroupId},
            </if>
            <if test="isDelete != null">
                  t.is_delete = #{IsDelete},
            </if>
            <if test="memberId != null">
                  t.member_id = #{MemberId},
            </if>
            <if test="operator != null">
                  t.operator = #{Operator},
            </if>
            <if test="status != null">
                  t.status = #{Status}
            </if>
            t.update_time= current_timestamp
        </set>
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </update>
    <select id="getDetectGoupUserById" resultType="com.railway.pojo.entity.DetectGoupUser">
          select
             t.create_time,
             t.group_id,
             t.id,
             t.is_delete,
             t.member_id,
             t.operator,
             t.status,
             t.update_time
          form detect_goup_user t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </select>
    <select id="getDetectGoupUserList" resultType="com.railway.pojo.entity.DetectGoupUser" parameterType="com.railway.pojo.req.GetDetectGoupUserListReq">
          select
             t.create_time,
             t.group_id,
             t.id,
             t.is_delete,
             t.member_id,
             t.operator,
             t.status,
             t.update_time
          form detect_goup_user t
          <where>
            <if test="createTimeStart !=null and  createTimeEnd != null">
                  t.create_time between #{createTimeStart} and #{createTimeEnd}
            </if>
            <if test="groupId != null">
                  and t.group_id = #{groupId}
            </if>
            <if test="isDelete != null">
                  and t.is_delete = #{isDelete}
            </if>
            <if test="memberId != null">
                  and t.member_id = #{memberId}
            </if>
            <if test="operator != null">
                  and t.operator = #{operator}
            </if>
            <if test="status != null">
                  and t.status = #{status}
            </if>
            <if test="updateTimeStart !=null and  updateTimeEnd != null">
                  and t.update_time between #{updateTimeStart} and #{updateTimeEnd}
            </if>
          </where>
    </select>
</mapper>
