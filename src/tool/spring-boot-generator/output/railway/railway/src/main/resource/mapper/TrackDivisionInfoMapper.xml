<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.railway.mapper.TrackDivisionInfoMapper">
    <insert id="addTrackDivisionInfo" parameterType="com.railway.pojo.req.AddTrackDivisionInfoReq">
          insert  into track_division_info t
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="bureauId != null">
                  t.bureau_id,
            </if>
             t.full_name,
            <if test="inputCode != null">
                  t.input_code,
            </if>
             t.is_delete,
            <if test="operator != null">
                  t.operator,
            </if>
            <if test="shortName != null">
                  t.short_name,
            </if>
             t.create_time
          </trim>
          values
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="bureauId != null">
                  #{bureauId},
            </if>
             #{fullName},
            <if test="inputCode != null">
                  #{inputCode},
            </if>
             #{isDelete},
            <if test="operator != null">
                  #{operator},
            </if>
            <if test="shortName != null">
                  #{shortName},
            </if>
             current_timestamp
          </trim>
    </insert>
    <delete id="deleteTrackDivisionInfoById">
          delete from track_division_info t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </delete>
    <update id="updateTrackDivisionInfoById" parameterType="com.railway.pojo.param.UpdateTrackDivisionInfoByIdParams">
        update track_division_info t
        <set>
            <if test="bureauId != null">
                  t.bureau_id = #{BureauId},
            </if>
            <if test="fullName != null">
                  t.full_name = #{FullName},
            </if>
            <if test="inputCode != null">
                  t.input_code = #{InputCode},
            </if>
            <if test="isDelete != null">
                  t.is_delete = #{IsDelete},
            </if>
            <if test="operator != null">
                  t.operator = #{Operator},
            </if>
            <if test="shortName != null">
                  t.short_name = #{ShortName}
            </if>
            t.update_time= current_timestamp
        </set>
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </update>
    <select id="getTrackDivisionInfoById" resultType="com.railway.pojo.entity.TrackDivisionInfo">
          select
             t.bureau_id,
             t.create_time,
             t.full_name,
             t.id,
             t.input_code,
             t.is_delete,
             t.operator,
             t.short_name,
             t.update_time
          form track_division_info t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </select>
    <select id="getTrackDivisionInfoList" resultType="com.railway.pojo.entity.TrackDivisionInfo" parameterType="com.railway.pojo.req.GetTrackDivisionInfoListReq">
          select
             t.bureau_id,
             t.create_time,
             t.full_name,
             t.id,
             t.input_code,
             t.is_delete,
             t.operator,
             t.short_name,
             t.update_time
          form track_division_info t
          <where>
            <if test="bureauId != null">
                  t.bureau_id = #{bureauId}
            </if>
            <if test="createTimeStart !=null and  createTimeEnd != null">
                  and t.create_time between #{createTimeStart} and #{createTimeEnd}
            </if>
            <if test="inputCode != null and inputCode != ''">
                  and t.input_code = #{inputCode}
            </if>
            <if test="isDelete != null">
                  and t.is_delete = #{isDelete}
            </if>
            <if test="operator != null">
                  and t.operator = #{operator}
            </if>
            <if test="updateTimeStart !=null and  updateTimeEnd != null">
                  and t.update_time between #{updateTimeStart} and #{updateTimeEnd}
            </if>
          </where>
    </select>
</mapper>
