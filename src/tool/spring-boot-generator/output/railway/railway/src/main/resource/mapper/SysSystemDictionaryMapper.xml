<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.railway.mapper.SysSystemDictionaryMapper">
    <insert id="addSysSystemDictionary" parameterType="com.railway.pojo.req.AddSysSystemDictionaryReq">
          insert  into sys_system_dictionary t
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="enumName != null">
                  t.enum_name,
            </if>
             t.enum_type,
             t.sort_id,
             t.status,
            <if test="systemId != null">
                  t.system_id,
            </if>
             t.text,
             t.value
          </trim>
          values
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="enumName != null">
                  #{enumName},
            </if>
             #{enumType},
             #{sortId},
             #{status},
            <if test="systemId != null">
                  #{systemId},
            </if>
             #{text},
             #{value}
          </trim>
    </insert>
    <delete id="deleteSysSystemDictionaryByDicid">
          delete from sys_system_dictionary t
          <where>
            <if test="dicid != null">
                  t.dicid = #{dicid}
            </if>
          </where>
    </delete>
    <update id="updateSysSystemDictionaryByDicid" parameterType="com.railway.pojo.param.UpdateSysSystemDictionaryByDicidParams">
        update sys_system_dictionary t
        <set>
            <if test="enumName != null">
                  t.enum_name = #{EnumName},
            </if>
            <if test="enumType != null">
                  t.enum_type = #{EnumType},
            </if>
            <if test="sortId != null">
                  t.sort_id = #{SortId},
            </if>
            <if test="status != null">
                  t.status = #{Status},
            </if>
            <if test="systemId != null">
                  t.system_id = #{SystemId},
            </if>
            <if test="text != null">
                  t.text = #{Text},
            </if>
            <if test="value != null">
                  t.value = #{Value}
            </if>
        </set>
          <where>
            <if test="dicid != null">
                  t.dicid = #{dicid}
            </if>
          </where>
    </update>
    <select id="getSysSystemDictionaryByDicid" resultType="com.railway.pojo.entity.SysSystemDictionary">
          select
             t.dicid,
             t.enum_name,
             t.enum_type,
             t.sort_id,
             t.status,
             t.system_id,
             t.text,
             t.value
          form sys_system_dictionary t
          <where>
            <if test="dicid != null">
                  t.dicid = #{dicid}
            </if>
          </where>
    </select>
    <select id="getSysSystemDictionaryList" resultType="com.railway.pojo.entity.SysSystemDictionary" parameterType="com.railway.pojo.req.GetSysSystemDictionaryListReq">
          select
             t.dicid,
             t.enum_name,
             t.enum_type,
             t.sort_id,
             t.status,
             t.system_id,
             t.text,
             t.value
          form sys_system_dictionary t
          <where>
            <if test="enumType != null">
                  t.enum_type = #{enumType}
            </if>
            <if test="sortId != null">
                  and t.sort_id = #{sortId}
            </if>
            <if test="status != null">
                  and t.status = #{status}
            </if>
            <if test="systemId != null">
                  and t.system_id = #{systemId}
            </if>
            <if test="value != null">
                  and t.value = #{value}
            </if>
          </where>
    </select>
</mapper>
