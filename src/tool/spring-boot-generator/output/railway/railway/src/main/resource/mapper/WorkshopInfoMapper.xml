<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.railway.mapper.WorkshopInfoMapper">
    <insert id="addWorkshopInfo" parameterType="com.railway.pojo.req.AddWorkshopInfoReq">
          insert  into workshop_info t
          <trim prefix="(" suffix=")" suffixOverrides=",">
             t.full_name,
            <if test="inputCode != null">
                  t.input_code,
            </if>
             t.is_delete,
            <if test="lineId != null">
                  t.line_id,
            </if>
            <if test="operator != null">
                  t.operator,
            </if>
            <if test="shortName != null">
                  t.short_name,
            </if>
            <if test="trackDivisionId != null">
                  t.track_division_id,
            </if>
            <if test="workshopType != null">
                  t.workshop_type,
            </if>
             t.create_time
          </trim>
          values
          <trim prefix="(" suffix=")" suffixOverrides=",">
             #{fullName},
            <if test="inputCode != null">
                  #{inputCode},
            </if>
             #{isDelete},
            <if test="lineId != null">
                  #{lineId},
            </if>
            <if test="operator != null">
                  #{operator},
            </if>
            <if test="shortName != null">
                  #{shortName},
            </if>
            <if test="trackDivisionId != null">
                  #{trackDivisionId},
            </if>
            <if test="workshopType != null">
                  #{workshopType},
            </if>
             current_timestamp
          </trim>
    </insert>
    <delete id="deleteWorkshopInfoById">
          delete from workshop_info t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </delete>
    <update id="updateWorkshopInfoById" parameterType="com.railway.pojo.param.UpdateWorkshopInfoByIdParams">
        update workshop_info t
        <set>
            <if test="fullName != null">
                  t.full_name = #{FullName},
            </if>
            <if test="inputCode != null">
                  t.input_code = #{InputCode},
            </if>
            <if test="isDelete != null">
                  t.is_delete = #{IsDelete},
            </if>
            <if test="lineId != null">
                  t.line_id = #{LineId},
            </if>
            <if test="operator != null">
                  t.operator = #{Operator},
            </if>
            <if test="shortName != null">
                  t.short_name = #{ShortName},
            </if>
            <if test="trackDivisionId != null">
                  t.track_division_id = #{TrackDivisionId},
            </if>
            <if test="workshopType != null">
                  t.workshop_type = #{WorkshopType}
            </if>
            t.update_time= current_timestamp
        </set>
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </update>
    <select id="getWorkshopInfoById" resultType="com.railway.pojo.entity.WorkshopInfo">
          select
             t.create_time,
             t.full_name,
             t.id,
             t.input_code,
             t.is_delete,
             t.line_id,
             t.operator,
             t.short_name,
             t.track_division_id,
             t.update_time,
             t.workshop_type
          form workshop_info t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </select>
    <select id="getWorkshopInfoList" resultType="com.railway.pojo.entity.WorkshopInfo" parameterType="com.railway.pojo.req.GetWorkshopInfoListReq">
          select
             t.create_time,
             t.full_name,
             t.id,
             t.input_code,
             t.is_delete,
             t.line_id,
             t.operator,
             t.short_name,
             t.track_division_id,
             t.update_time,
             t.workshop_type
          form workshop_info t
          <where>
            <if test="createTimeStart !=null and  createTimeEnd != null">
                  t.create_time between #{createTimeStart} and #{createTimeEnd}
            </if>
            <if test="inputCode != null and inputCode != ''">
                  and t.input_code = #{inputCode}
            </if>
            <if test="isDelete != null">
                  and t.is_delete = #{isDelete}
            </if>
            <if test="lineId != null">
                  and t.line_id = #{lineId}
            </if>
            <if test="operator != null">
                  and t.operator = #{operator}
            </if>
            <if test="trackDivisionId != null">
                  and t.track_division_id = #{trackDivisionId}
            </if>
            <if test="updateTimeStart !=null and  updateTimeEnd != null">
                  and t.update_time between #{updateTimeStart} and #{updateTimeEnd}
            </if>
            <if test="workshopType != null">
                  and t.workshop_type = #{workshopType}
            </if>
          </where>
    </select>
</mapper>
