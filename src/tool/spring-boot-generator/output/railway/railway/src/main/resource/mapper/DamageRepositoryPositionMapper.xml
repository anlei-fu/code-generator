<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.railway.mapper.DamageRepositoryPositionMapper">
    <insert id="addDamageRepositoryPosition" parameterType="com.railway.pojo.req.AddDamageRepositoryPositionReq">
          insert  into damage_repository_position t
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="bridgeTunnel != null">
                  t.bridge_tunnel,
            </if>
            <if test="damagePosition != null">
                  t.damage_position,
            </if>
            <if test="direction != null">
                  t.direction,
            </if>
            <if test="holePosition != null">
                  t.hole_position,
            </if>
            <if test="lineCategory != null">
                  t.line_category,
            </if>
            <if test="lineId != null">
                  t.line_id,
            </if>
            <if test="lineName != null">
                  t.line_name,
            </if>
            <if test="miles != null">
                  t.miles,
            </if>
            <if test="stationId != null">
                  t.station_id,
            </if>
            <if test="stockExchange != null">
                  t.stock_exchange,
            </if>
            <if test="thighTrackId != null">
                  t.thigh_track_id,
            </if>
             t.create_time
          </trim>
          values
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="bridgeTunnel != null">
                  #{bridgeTunnel},
            </if>
            <if test="damagePosition != null">
                  #{damagePosition},
            </if>
            <if test="direction != null">
                  #{direction},
            </if>
            <if test="holePosition != null">
                  #{holePosition},
            </if>
            <if test="lineCategory != null">
                  #{lineCategory},
            </if>
            <if test="lineId != null">
                  #{lineId},
            </if>
            <if test="lineName != null">
                  #{lineName},
            </if>
            <if test="miles != null">
                  #{miles},
            </if>
            <if test="stationId != null">
                  #{stationId},
            </if>
            <if test="stockExchange != null">
                  #{stockExchange},
            </if>
            <if test="thighTrackId != null">
                  #{thighTrackId},
            </if>
             current_timestamp
          </trim>
    </insert>
    <delete id="deleteDamageRepositoryPositionByOrderDetailId">
          delete from damage_repository_position t
          <where>
            <if test="orderDetailId != null">
                  t.order_detail_id = #{orderDetailId}
            </if>
          </where>
    </delete>
    <update id="updateDamageRepositoryPositionByOrderDetailId" parameterType="com.railway.pojo.param.UpdateDamageRepositoryPositionByOrderDetailIdParams">
        update damage_repository_position t
        <set>
            <if test="bridgeTunnel != null">
                  t.bridge_tunnel = #{BridgeTunnel},
            </if>
            <if test="damagePosition != null">
                  t.damage_position = #{DamagePosition},
            </if>
            <if test="direction != null">
                  t.direction = #{Direction},
            </if>
            <if test="holePosition != null">
                  t.hole_position = #{HolePosition},
            </if>
            <if test="lineCategory != null">
                  t.line_category = #{LineCategory},
            </if>
            <if test="lineId != null">
                  t.line_id = #{LineId},
            </if>
            <if test="lineName != null">
                  t.line_name = #{LineName},
            </if>
            <if test="miles != null">
                  t.miles = #{Miles},
            </if>
            <if test="stationId != null">
                  t.station_id = #{StationId},
            </if>
            <if test="stockExchange != null">
                  t.stock_exchange = #{StockExchange},
            </if>
            <if test="thighTrackId != null">
                  t.thigh_track_id = #{ThighTrackId}
            </if>
            t.update_time= current_timestamp
        </set>
          <where>
            <if test="orderDetailId != null">
                  t.order_detail_id = #{orderDetailId}
            </if>
          </where>
    </update>
    <select id="getDamageRepositoryPositionByOrderDetailId" resultType="com.railway.pojo.entity.DamageRepositoryPosition">
          select
             t.bridge_tunnel,
             t.create_time,
             t.damage_position,
             t.direction,
             t.hole_position,
             t.line_category,
             t.line_id,
             t.line_name,
             t.miles,
             t.order_detail_id,
             t.station_id,
             t.stock_exchange,
             t.thigh_track_id,
             t.update_time
          form damage_repository_position t
          <where>
            <if test="orderDetailId != null">
                  t.order_detail_id = #{orderDetailId}
            </if>
          </where>
    </select>
    <select id="getDamageRepositoryPositionList" resultType="com.railway.pojo.entity.DamageRepositoryPosition" parameterType="com.railway.pojo.req.GetDamageRepositoryPositionListReq">
          select
             t.bridge_tunnel,
             t.create_time,
             t.damage_position,
             t.direction,
             t.hole_position,
             t.line_category,
             t.line_id,
             t.line_name,
             t.miles,
             t.order_detail_id,
             t.station_id,
             t.stock_exchange,
             t.thigh_track_id,
             t.update_time
          form damage_repository_position t
          <where>
            <if test="bridgeTunnel != null">
                  t.bridge_tunnel = #{bridgeTunnel}
            </if>
            <if test="createTimeStart !=null and  createTimeEnd != null">
                  and t.create_time between #{createTimeStart} and #{createTimeEnd}
            </if>
            <if test="damagePosition != null">
                  and t.damage_position = #{damagePosition}
            </if>
            <if test="direction != null">
                  and t.direction = #{direction}
            </if>
            <if test="holePosition != null">
                  and t.hole_position = #{holePosition}
            </if>
            <if test="lineCategory != null">
                  and t.line_category = #{lineCategory}
            </if>
            <if test="lineId != null">
                  and t.line_id = #{lineId}
            </if>
            <if test="miles != null">
                  and t.miles = #{miles}
            </if>
            <if test="stationId != null">
                  and t.station_id = #{stationId}
            </if>
            <if test="stockExchange != null">
                  and t.stock_exchange = #{stockExchange}
            </if>
            <if test="thighTrackId != null">
                  and t.thigh_track_id = #{thighTrackId}
            </if>
            <if test="updateTimeStart !=null and  updateTimeEnd != null">
                  and t.update_time between #{updateTimeStart} and #{updateTimeEnd}
            </if>
          </where>
    </select>
</mapper>
