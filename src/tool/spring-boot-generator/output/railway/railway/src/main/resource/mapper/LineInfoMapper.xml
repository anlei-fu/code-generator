            </if>
            <if test="lineCode != null">
                  and t.line_code = #{lineCode}
            </if>
            <if test="lineNum != null">
                  and t.line_num = #{lineNum}
            </if>
            <if test="minRadiusCurve != null">
                  and t.min_radius_curve = #{minRadiusCurve}
            </if>
            <if test="nature != null">
                  and t.nature = #{nature}
            </if>
            <if test="operateType != null">
