                  t.status = #{Status},
            </if>
            <if test="trackType != null">
                  t.track_type = #{TrackType},
            </if>
            <if test="tractionType != null">
                  t.traction_type = #{TractionType},
            </if>
            <if test="yearVolume != null">
                  t.year_volume = #{YearVolume}
            </if>
            t.update_time= current_timestamp
        </set>
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </update>
    <select id="getLineInfoById" resultType="com.railway.pojo.entity.LineInfo">
          select
