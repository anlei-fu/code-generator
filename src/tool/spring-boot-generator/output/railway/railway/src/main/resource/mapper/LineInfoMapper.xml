            </if>
            <if test="operator != null">
                  and t.operator = #{operator}
            </if>
            <if test="status != null">
                  and t.status = #{status}
            </if>
            <if test="trackType != null">
                  and t.track_type = #{trackType}
            </if>
            <if test="tractionType != null">
                  and t.traction_type = #{tractionType}
            </if>
            <if test="updateTimeStart !=null and  updateTimeEnd != null">
                  and t.update_time between #{updateTimeStart} and #{updateTimeEnd}
            </if>
            <if test="yearVolume != null">
                  and t.year_volume = #{yearVolume}
            </if>
          </where>
    </select>
</mapper>
