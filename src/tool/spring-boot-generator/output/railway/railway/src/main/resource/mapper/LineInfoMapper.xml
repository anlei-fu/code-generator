             t.short_name,
             t.status,
             t.track_type,
             t.traction_type,
             t.update_time,
             t.year_volume
          form line_info t
          <where>
            <if test="ballastType != null">
                  t.ballast_type = #{ballastType}
            </if>
            <if test="commissioningDateStart !=null and  commissioningDateEnd != null">
                  and t.commissioning_date between #{commissioningDateStart} and #{commissioningDateEnd}
            </if>
            <if test="createTimeStart !=null and  createTimeEnd != null">
                  and t.create_time between #{createTimeStart} and #{createTimeEnd}
            </if>
            <if test="distance != null">
                  and t.distance = #{distance}
            </if>
