<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.railway.mapper.LineInfoMapper">
    <insert id="addLineInfo" parameterType="com.railway.pojo.req.AddLineInfoReq">
          insert  into line_info t
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="ballastType != null">
                  t.ballast_type,
            </if>
            <if test="commissioningDate != null">
                  t.commissioning_date,
            </if>
            <if test="distance != null">
                  t.distance,
            </if>
             t.full_name,
            <if test="gaugeType != null">
                  t.gauge_type,
            </if>
            <if test="grade != null">
                  t.grade,
            </if>
             t.input_code,
             t.is_delete,
            <if test="limitGrad != null">
                  t.limit_grad,
            </if>
            <if test="lineBranches != null">
                  t.line_branches,
            </if>
             t.line_code,
            <if test="lineNum != null">
                  t.line_num,
            </if>
            <if test="minRadiusCurve != null">
                  t.min_radius_curve,
            </if>
            <if test="nature != null">
                  t.nature,
            </if>
            <if test="operateType != null">
                  t.operate_type,
            </if>
            <if test="operator != null">
                  t.operator,
            </if>
             t.short_name,
            <if test="status != null">
                  t.status,
            </if>
            <if test="trackType != null">
                  t.track_type,
            </if>
            <if test="tractionType != null">
                  t.traction_type,
            </if>
            <if test="yearVolume != null">
                  t.year_volume,
            </if>
             t.create_time
          </trim>
          values
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="ballastType != null">
                  #{ballastType},
            </if>
            <if test="commissioningDate != null">
                  #{commissioningDate},
            </if>
            <if test="distance != null">
                  #{distance},
            </if>
             #{fullName},
            <if test="gaugeType != null">
                  #{gaugeType},
            </if>
            <if test="grade != null">
                  #{grade},
            </if>
             #{inputCode},
             #{isDelete},
            <if test="limitGrad != null">
                  #{limitGrad},
            </if>
            <if test="lineBranches != null">
                  #{lineBranches},
            </if>
             #{lineCode},
            <if test="lineNum != null">
                  #{lineNum},
            </if>
            <if test="minRadiusCurve != null">
                  #{minRadiusCurve},
            </if>
            <if test="nature != null">
                  #{nature},
            </if>
            <if test="operateType != null">
                  #{operateType},
            </if>
            <if test="operator != null">
                  #{operator},
            </if>
             #{shortName},
            <if test="status != null">
                  #{status},
            </if>
            <if test="trackType != null">
                  #{trackType},
            </if>
            <if test="tractionType != null">
                  #{tractionType},
            </if>
            <if test="yearVolume != null">
                  #{yearVolume},
            </if>
             current_timestamp
          </trim>
    </insert>
    <delete id="deleteLineInfoById">
          delete from line_info t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </delete>
    <update id="updateLineInfoById" parameterType="com.railway.pojo.param.UpdateLineInfoByIdParams">
        update line_info t
        <set>
            <if test="ballastType != null">
                  t.ballast_type = #{BallastType},
            </if>
            <if test="commissioningDate != null">
                  t.commissioning_date = #{CommissioningDate},
            </if>
            <if test="distance != null">
                  t.distance = #{Distance},
            </if>
            <if test="fullName != null">
                  t.full_name = #{FullName},
            </if>
            <if test="gaugeType != null">
                  t.gauge_type = #{GaugeType},
            </if>
            <if test="grade != null">
                  t.grade = #{Grade},
            </if>
            <if test="inputCode != null">
                  t.input_code = #{InputCode},
            </if>
            <if test="isDelete != null">
                  t.is_delete = #{IsDelete},
            </if>
            <if test="limitGrad != null">
                  t.limit_grad = #{LimitGrad},
            </if>
            <if test="lineBranches != null">
                  t.line_branches = #{LineBranches},
            </if>
            <if test="lineCode != null">
