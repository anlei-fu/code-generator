             t.commissioning_date,
             t.create_time,
             t.distance,
             t.full_name,
             t.gauge_type,
             t.grade,
             t.id,
             t.input_code,
             t.is_delete,
             t.limit_grad,
             t.line_branches,
             t.line_code,
             t.line_num,
             t.min_radius_curve,
             t.nature,
             t.operate_type,
             t.operator,
             t.short_name,
             t.status,
             t.track_type,
             t.traction_type,
             t.update_time,
             t.year_volume
          form line_info t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </select>
    <select id="getLineInfoList" resultType="com.railway.pojo.entity.LineInfo" parameterType="com.railway.pojo.req.GetLineInfoListReq">
          select
             t.ballast_type,
             t.commissioning_date,
             t.create_time,
             t.distance,
             t.full_name,
             t.gauge_type,
             t.grade,
             t.id,
             t.input_code,
             t.is_delete,
             t.limit_grad,
             t.line_branches,
             t.line_code,
             t.line_num,
             t.min_radius_curve,
             t.nature,
             t.operate_type,
