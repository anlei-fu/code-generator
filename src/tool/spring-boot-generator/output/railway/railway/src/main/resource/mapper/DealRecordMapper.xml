<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.railway.mapper.DealRecordMapper">
    <insert id="addDealRecord" parameterType="com.railway.pojo.req.AddDealRecordReq">
          insert  into deal_record t
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="cmdNo != null">
                  t.cmd_no,
            </if>
            <if test="dealRemark != null">
                  t.deal_remark,
            </if>
            <if test="dealResult != null">
                  t.deal_result,
            </if>
            <if test="dealTime != null">
                  t.deal_time,
            </if>
            <if test="deleteAuditStatus != null">
                  t.delete_audit_status,
            </if>
            <if test="deleteTime != null">
                  t.delete_time,
            </if>
             t.is_delete,
            <if test="operator != null">
                  t.operator,
            </if>
            <if test="orderDetailId != null">
                  t.order_detail_id,
            </if>
            <if test="workareaId != null">
                  t.workarea_id,
            </if>
            <if test="workshopInfoId != null">
                  t.workshop_info_id,
            </if>
             t.create_time
          </trim>
          values
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="cmdNo != null">
                  #{cmdNo},
            </if>
            <if test="dealRemark != null">
                  #{dealRemark},
            </if>
            <if test="dealResult != null">
                  #{dealResult},
            </if>
            <if test="dealTime != null">
                  #{dealTime},
            </if>
            <if test="deleteAuditStatus != null">
                  #{deleteAuditStatus},
            </if>
            <if test="deleteTime != null">
                  #{deleteTime},
            </if>
             #{isDelete},
            <if test="operator != null">
                  #{operator},
            </if>
            <if test="orderDetailId != null">
                  #{orderDetailId},
            </if>
            <if test="workareaId != null">
                  #{workareaId},
            </if>
            <if test="workshopInfoId != null">
                  #{workshopInfoId},
            </if>
             current_timestamp
          </trim>
    </insert>
    <delete id="deleteDealRecordById">
          delete from deal_record t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </delete>
    <update id="updateDealRecordById" parameterType="com.railway.pojo.param.UpdateDealRecordByIdParams">
        update deal_record t
        <set>
            <if test="cmdNo != null">
                  t.cmd_no = #{CmdNo},
            </if>
            <if test="dealRemark != null">
                  t.deal_remark = #{DealRemark},
            </if>
            <if test="dealResult != null">
                  t.deal_result = #{DealResult},
            </if>
            <if test="dealTime != null">
                  t.deal_time = #{DealTime},
            </if>
            <if test="deleteAuditStatus != null">
                  t.delete_audit_status = #{DeleteAuditStatus},
            </if>
            <if test="deleteTime != null">
                  t.delete_time = #{DeleteTime},
            </if>
            <if test="isDelete != null">
                  t.is_delete = #{IsDelete},
            </if>
            <if test="operator != null">
                  t.operator = #{Operator},
            </if>
            <if test="orderDetailId != null">
                  t.order_detail_id = #{OrderDetailId},
            </if>
            <if test="workareaId != null">
                  t.workarea_id = #{WorkareaId},
            </if>
            <if test="workshopInfoId != null">
                  t.workshop_info_id = #{WorkshopInfoId}
            </if>
            t.update_time= current_timestamp
        </set>
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </update>
    <select id="getDealRecordById" resultType="com.railway.pojo.entity.DealRecord">
          select
             t.cmd_no,
             t.create_time,
             t.deal_remark,
             t.deal_result,
             t.deal_time,
             t.delete_audit_status,
             t.delete_time,
             t.id,
             t.is_delete,
             t.operator,
             t.order_detail_id,
             t.update_time,
             t.workarea_id,
             t.workshop_info_id
          form deal_record t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </select>
    <select id="getDealRecordList" resultType="com.railway.pojo.entity.DealRecord" parameterType="com.railway.pojo.req.GetDealRecordListReq">
          select
             t.cmd_no,
             t.create_time,
             t.deal_remark,
             t.deal_result,
             t.deal_time,
             t.delete_audit_status,
             t.delete_time,
             t.id,
             t.is_delete,
             t.operator,
             t.order_detail_id,
             t.update_time,
             t.workarea_id,
             t.workshop_info_id
          form deal_record t
          <where>
            <if test="cmdNo != null and cmdNo != ''">
                  t.cmd_no = #{cmdNo}
            </if>
            <if test="createTimeStart !=null and  createTimeEnd != null">
                  and t.create_time between #{createTimeStart} and #{createTimeEnd}
            </if>
            <if test="dealResult != null">
                  and t.deal_result = #{dealResult}
            </if>
            <if test="dealTimeStart !=null and  dealTimeEnd != null">
                  and t.deal_time between #{dealTimeStart} and #{dealTimeEnd}
            </if>
            <if test="deleteAuditStatus != null">
                  and t.delete_audit_status = #{deleteAuditStatus}
            </if>
            <if test="deleteTimeStart !=null and  deleteTimeEnd != null">
                  and t.delete_time between #{deleteTimeStart} and #{deleteTimeEnd}
            </if>
            <if test="isDelete != null">
                  and t.is_delete = #{isDelete}
            </if>
            <if test="operator != null">
                  and t.operator = #{operator}
            </if>
            <if test="orderDetailId != null">
                  and t.order_detail_id = #{orderDetailId}
            </if>
            <if test="updateTimeStart !=null and  updateTimeEnd != null">
                  and t.update_time between #{updateTimeStart} and #{updateTimeEnd}
            </if>
            <if test="workareaId != null">
                  and t.workarea_id = #{workareaId}
            </if>
            <if test="workshopInfoId != null">
                  and t.workshop_info_id = #{workshopInfoId}
            </if>
          </where>
    </select>
</mapper>
