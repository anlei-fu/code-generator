<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.contract.mapper.SystemDictionaryMapper">
    <insert id="addSystemDictionary" parameterType="com.contract.pojo.req.AddSystemDictionaryReq">
          insert  into system_dictionary t
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="remark != null">
                  t.remark,
            </if>
            <if test="sortNum != null">
                  t.sort_num,
            </if>
            <if test="type != null">
                  t.type,
            </if>
            <if test="value != null">
                  t.value
            </if>
          </trim>
          values
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="remark != null">
                  #{remark},
            </if>
            <if test="sortNum != null">
                  #{sortNum},
            </if>
            <if test="type != null">
                  #{type},
            </if>
            <if test="value != null">
                  #{value}
            </if>
          </trim>
    </insert>
    <delete id="deleteSystemDictionaryById">
          delete from system_dictionary t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </delete>
    <update id="updateSystemDictionaryById" parameterType="com.contract.pojo.param.UpdateSystemDictionaryByIdParams">
        update system_dictionary t
        <set>
            <if test="remark != null">
                  t.remark = #{Remark},
            </if>
            <if test="sortNum != null">
                  t.sort_num = #{SortNum},
            </if>
            <if test="type != null">
                  t.type = #{Type},
            </if>
            <if test="value != null">
                  t.value = #{Value}
            </if>
        </set>
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </update>
    <select id="getSystemDictionaryById" resultType="com.contract.pojo.entity.SystemDictionary">
          select
             t.id,
             t.remark,
             t.sort_num,
             t.type,
             t.value
          form system_dictionary t
          <where>
            <if test="id != null">
                  t.id = #{id}
            </if>
          </where>
    </select>
    <select id="getSystemDictionaryList" resultType="com.contract.pojo.entity.SystemDictionary" parameterType="com.contract.pojo.req.GetSystemDictionaryListReq">
          select
             t.id,
             t.remark,
             t.sort_num,
             t.type,
             t.value
          form system_dictionary t
          <where>
            <if test="sortNum != null">
                  t.sort_num = #{sortNum}
            </if>
            <if test="type != null and type != ''">
                  and t.type = #{type}
            </if>
            <if test="value != null and value != ''">
                  and t.value = #{value}
            </if>
          </where>
    </select>
</mapper>
