<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spider.mapper.WorkerRunRecordMapper">
    <insert id="addWorkerRunRecord" parameterType="com.spider.pojo.req.AddWorkerRunRecordReq">
          insert  into worker_run_record
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="startTime != null">
                  worker_run_record.start_time,
            </if>
            <if test="endTime != null">
                  worker_run_record.end_time
            </if>
          </trim>
          values
          <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="startTime != null">
                  #{startTime},
            </if>
            <if test="endTime != null">
                  #{endTime}
            </if>
          </trim>
    </insert>
    <delete id="deleteWorkerRunRecordById">
          delete from worker_run_record
          <where>
            <if test="id != null">
                  worker_run_record.id = #{id}
            </if>
          </where>
    </delete>
    <update id="updateWorkerRunRecordById" parameterType="com.spider.pojo.param.UpdateWorkerRunRecordByIdParams">
        update worker_run_record
        <set>
            <if test="startTime != null">
                  worker_run_record.start_time = #{StartTime},
            </if>
            <if test="endTime != null">
                  worker_run_record.end_time = #{EndTime}
            </if>
        </set>
          <where>
            <if test="id != null">
                  worker_run_record.id = #{id}
            </if>
          </where>
    </update>
    <select id="getWorkerRunRecordById" resultType="com.spider.pojo.entity.WorkerRunRecord">
          select
             worker_run_record.id,
             worker_run_record.start_time,
             worker_run_record.end_time
          form worker_run_record
          <where>
            <if test="id != null">
                  worker_run_record.id = #{id}
            </if>
          </where>
    </select>
    <select id="getWorkerRunRecordList" resultType="com.spider.pojo.entity.WorkerRunRecord" parameterType="com.spider.pojo.req.GetWorkerRunRecordListReq">
          select
             worker_run_record.id,
             worker_run_record.start_time,
             worker_run_record.end_time
          form worker_run_record
          <where>
            <if test="startTime != null">
                  worker_run_record.start_time = #{startTime}
            </if>
            <if test="endTime != null">
                  and worker_run_record.end_time = #{endTime}
            </if>
          </where>
    </select>
</mapper>
