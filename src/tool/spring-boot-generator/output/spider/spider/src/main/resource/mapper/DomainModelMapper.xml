<mapper namespace="com.spider.mapper.DomainModelMapper">
    <insert id="addDomainModel">
          insert  into domain_model
          <trim prefix="(" suffix=")" suffixOverrides=",">
 domain_model.task_id,
 domain_model.domain_id,
 domain_model.model,

          </trim>
          values
          <trim prefix="(" suffix=")" suffixOverrides=",">
 #{taskId} ,
 #{domainId} ,
 #{model} ,

          </trim>
    </insert>
    <delete id="deleteDomainModelById">
          delete from domain_model
@where
    </delete>
    <update id="updateDomainModelById">
        update domain_model
@set
@where
    </update>
    <select id="getDomainModelById" resultType="com.spider.pojo.@resultType">
          select
undefined
          form domainModelName

@where
    </select>
    <select id="getDomainModelList" resultType="com.spider.pojo.@resultType">
