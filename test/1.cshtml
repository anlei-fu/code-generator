        </tr>
        <tr id="need_invoice1">
                <td>营业厅开发票</td>
              <td> 
                      @Html.DropDownList("needinvoice", new SelectList(MatchingRecharge.UserService.DataService.Instance.GetDataList("HallBillType"), "value", "name"), "--请选择--", new { style = "width:80px" })
                </td>
        </tr>
        <tr id="Source_Type1">
